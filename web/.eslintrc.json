{
    "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
    "rules": {
            "no-console": 1
    },
    "plugin": ["import"],
    "parserOptions": {
            "ecmaVersion": 2018,
            "sourceType": "module"
    },
    "env": { "es6": true, "browser": true, "node": true }
}